{
  "name": "clippster-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client": "cd client && yarn dev",
    "server": "cd server && mix phx.server",
    "tauri": "cd client && yarn tauri dev",
    "dev": "concurrently \"yarn server\" \"yarn tauri\" --names \"server,client\" --prefix-colors \"blue,magenta\""
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
